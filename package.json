	"description": "基于node开发的一个多端文件/文字同步工具",
	"author": "NoRain",
	"bin": "src/server/index.ts",
	"main": "src/server/index.ts",
	"scripts": {
		"build": "npx webpack --mode production",
		"watch": "npx webpack --mode development --watch",

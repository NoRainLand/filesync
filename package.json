{
	"name": "file-sync",
	"version": "1.5.0",
	"description": "一个简单的文件/文字同步服务器",
	"bin": "dist/index.js",
	"main": "dist/index.js",
	"scripts": {
		"dev": "nodemon ./dist/index.js",
		"watch": "concurrently \" tsc --project ./tsconfig.json -w\" \"tsc --project ./src/es6/tsconfig.es6.json -w\"",
		"copy": "concurrently \"@powershell copy src/es6/index.html dist/es6/ \" \" @powershell copy src/es6/favicon.ico dist/es6/ \" \" @powershell copy src/es6/pico.min.css dist/es6/ \" \" @powershell copy src/es6/qrcode.min.js dist/es6/ \"",
		"build": "concurrently \" tsc --project ./tsconfig.json\" \"tsc --project ./src/es6/tsconfig.es6.json\" ",
		"start": "node ./dist/index.js",
		"exec": "concurrently \"pkg -t win package.json\" "
	},
	"keywords": [
		"fileSnyc",
		"file",
		"sync",
		"server",
		"text",
		"textSync",
		"textSyncServer",
		"fileSyncServer",
		"fileSync",
		"textSyncServer"
	],
	"author": "NoRain",
	"license": "MIT",
	"devDependencies": {
		"@types/express": "^4.17.21",
		"@types/multer": "^1.4.11",
		"@types/node": "^20.2.1",
		"@types/socket.io": "^3.0.2",
		"@types/ws": "^8.5.10",
		"concurrently": "^8.2.2",
		"nodemon": "^2.0.22",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.0.4",
		"ws": "^8.15.1"
	},
	"dependencies": {
		"common-js": "^0.3.8",
		"express": "^4.18.2",
		"multer": "^1.4.5-lts.1",
		"socket.io": "^4.7.2",
		"socket.io-client": "^4.7.2",
		"sqlite": "^5.1.1",
		"sqlite3": "^5.1.6"
	},
	"pkg": {
		"assets": [
			"dist/**/*",
			"node_modules/sqlite3/**/*"
		],
		"outputPath": "exec"
	}
}

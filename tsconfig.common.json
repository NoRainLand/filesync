{
	"compilerOptions": {
		"target": "ES6",
		"module": "CommonJS",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"sourceMap": false,
		"outDir": "./dist/server_temp",
		"strictPropertyInitialization": false
	},
	"exclude": ["node_modules", "src/client/*.ts"],
	"include": ["src/server/*.ts", "src/common/*.ts", "src/ProjectConfig.ts"]
}

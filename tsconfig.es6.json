{
	"compilerOptions": {
		"target": "ES6",
		"module": "ES6",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"sourceMap": false,
		"outDir": "./dist/client_temp",
		"moduleResolution": "Node",
		"strictPropertyInitialization": false
	},
	"exclude": ["node_modules", "src/server/*.ts"],
	"include": ["src/client/*.ts", "src/common/*.ts", "src/ProjectConfig.ts"]
}
